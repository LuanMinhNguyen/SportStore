@model IEnumerable<string>
<div class="d-grid gap-2 col-6 max-auto">
    @*<button class="btn btn-primary" type="button"></button>
        <button class="btn btn-primary" type="button"></button>*@
    <a class="btn btn-primary" asp-action="List" asp-controller="Product" asp-route-category="">
        Home
    </a>
    @foreach (string category in Model)
            {
                <a class="btn btn-secondary" asp-action="List" asp-controller="Product" asp-route-category="@category">
                    @category
                </a>
            }
</div>
